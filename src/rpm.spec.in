Name: @PACKAGE_NAME@
Version:	@PACKAGE_VERSION@
Release:	@PACKAGE_RELEASE@%{?dist}
Summary:	JavaScript libraries for parsegraph.com.
Group:		Applications/Internet
License:	Proprietary
URL:		@PACKAGE_URL@
Source0:	@PACKAGE_NAME@-@PACKAGE_VERSION@.tar.gz
BuildRoot:	%(mktemp -ud %{_tmppath}/%{name}-%{version}-%{release}-XXXXXX)
BuildArch:	noarch

%description
@PACKAGE_DESCRIPTION@

%prep
%setup -q

%build

%install
mkdir -p %{buildroot}/%{_datarootdir}/parsegraph
cp *.js %{buildroot}/%{_datarootdir}/parsegraph

%files
%{_datarootdir}/parsegraph/*.js
